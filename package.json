    "build-server": "run-s clean-server compile-server",
    "clean-server": "rimraf server/build",
    "watch-client": "run-s clean-client watch-webpack",
    "watch-webpack": "tsc --esModuleInterop ./webpack/dev.config.ts && webpack --config ./webpack/dev.config.js --watch",
